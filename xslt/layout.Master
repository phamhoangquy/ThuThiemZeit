        <script type="text/javascript">
            function HideToolbar() {
                $(".cmsadminpanel").addClass("toggled"), $(".fa-chevron-circle-right").addClass("fa-rotate-180")
            }

            function ShowToolbar() {
                $(".cmsadminpanel").removeClass("toggled"), $(".fa-chevron-circle-right").removeClass("fa-rotate-180")
            }
			function HideForm() {
				$(".modal-ads").addClass("active"), $('.mask').addClass("active");
			}

			function ShowForm() {
				$(".modal-ads").removeClass("active"), $('.mask').removeClass("active")
			}

            $(document).ready(function() {
                $("#pullmenucms").click(function(o) {
                    o.preventDefault(), $(this).parents(".cmsadminpanel").hasClass("toggled") ? (
                        ShowToolbar(), Set_Cookie("toolbarOpenState", "open")) : (HideToolbar(),
                        Set_Cookie("toolbarOpenState", "closed"))
                });
                var o = Get_Cookie("toolbarOpenState");
                null != o && ("closed" == o && HideToolbar(), "open" == o && ShowToolbar())

				$(".modal-ads .close").click(function(formHide) {
					formHide.preventDefault(), 
					$(this).parents(".modal-ads").hasClass("active") ? 
					(ShowForm(), Set_Cookie("modalFormPopup", "open")) : 
					(HideForm(), Set_Cookie("modalFormPopup", "closed"))
				});
				var formHide = Get_Cookie("modalFormPopup");
				null != formHide && ("closed" == formHide && HideForm(), "open" == formHide && ShowForm())
            });
